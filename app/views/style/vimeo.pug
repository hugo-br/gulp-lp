style.
  .controls-wrap {
  position: absolute;
  width: 25%;
  left: 60%;
  top: 10%;
  height: 80%;
  }
  .controls-wrap:hover .video-controls,
  .controls-wrap:hover .sound-bar div {
  display: block;
  opacity: 1;
  }
  .video-controls {
  position: absolute;
  margin: -6em 0 0 -3.25em;
  position: absolute;
  top: 52%;
  left: 48%;
  z-index: 1;
  display: none;
  opacity: 0;
  transition: all 75ms;
  top: calc((100% + 50px) / 2);
  left: calc((100%) / 2);
  }
  .video-controls.hidden {
  display: none;
  opacity: 0;
  }
  .btn-controls.hidden {
  display: none;
  opacity: 0;
  }
  .btn-controls {
  /*Reset's every elements apperance*/
  background: none repeat scroll 0 0 transparent;
  border: medium none;
  border-spacing: 0;
  color: #26589F;
  font-family: 'PT Sans Narrow', sans-serif;
  font-size: 16px;
  font-weight: normal;
  line-height: 1.42rem;
  list-style: none outside none;
  margin: 0;
  padding: 0;
  text-align: left;
  text-decoration: none;
  text-indent: 0;
  opacity: 0.8;
  min-width: 0;
  transition: opacity 250ms ease-out, background-color 40ms, color 40ms;
  }
  button.btn-controls:after {
  content: none;
  }
  .btn-controls:hover {
  opacity: 1;
  }
  .sound-bar {
  position: absolute;
  top: 89%;
  left: 46%;
  width: 100%;
  height: 20%;
  text-align: right;
  }
  .sound-bar>div {
  display: block;
  opacity: 1;
  transition: opacity 250ms ease-out;
  }
  /* Play/Stop Buttons */
  .video-controls .btn-controls > img {
  display: block;
  margin:auto;
  }
  .video-controls .btn-controls {
  width: 150px;
  height: 150px;
  border: none;
  }
  /* Sound Buttons */
  .sound-bar .btn-controls {
  top: 50%;
  position: absolute;
  left: 90%;
  border: none;
  }
  .sound-bar:hover div {
  display: block;
  opacity: 1;
  }
  .sound-bar .btn-controls {
  width: 65px;
  height: 65px;
  min-width: 0px;
  }
  .embed-container {
  position: relative;
  padding-bottom: 56.25%;
  height: 0;
  overflow: hidden;
  max-width: 100%;
  }
  /* full-width video */
  .embed-container iframe {
  position: absolute;
  z-index: 0;
  width: 100%;
  height: 100%;
  }
  @media (min-width:769px){
  .tablet-video{display:none;}
  }
  @media (max-width:591px){
  .tablet-video{display: none;}
  }
